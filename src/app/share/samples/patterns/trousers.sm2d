        <increment description="* DO NOT CHANGE *&#10;Calculated:&#10;HipCircumference/20+3 + CrotchHeight&#10;&#10;&#10;" formula="hip_circ/20+3 + leg_crotch_to_floor" name="#HipLineHeight"/>
